@using Domain.EntitiesDto
@model SliderDto



<h3>Нест намудан</h3>
<form asp-action="Delete" asp-controller="Slider" asp-antiforgery="true" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly"></div>
    <div>

        <div class="form-group">
            <label asp-for="Title"><b>Сарлавха</b></label>
            <input type="text" asp-for="Title" class="form-control" />
            <span asp-validation-for="Title"></span>
        </div>
     
        
          
        <input asp-for="ImageName"? type="hidden"  class="form-control" accept="application" />
         
         <div class="form-group">
            <label asp-for="Enabled"><b>Фаъол</b></label>
            <input type="text" asp-for="Enabled" class="form-control" />
            <span asp-validation-for="Enabled"></span>
        </div>     

       
       <div class="card-footer">
                <center>
                    <button type="submit" class="btn btn-primary">Нест намудан</button>
                    <a asp-action="Index" asp-controller="Slider" class="btn btn-secondary">Бекор намудан</a>
                </center>
            </div>
    
    </div>
</form>