@using Domain.EntitiesDto
@model NewsDto



<h3>Шумо ин малумотро нест намудан мехохед ?</h3>
<form asp-action="Delete" asp-controller="News" asp-antiforgery="true" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly"></div>
    <div>

        <div class="form-group">
            <label asp-for="Title"><b>Сарлавха</b></label>
            <input type="text" asp-for="Title" class="form-control" />
            <span asp-validation-for="Title"></span>
        </div>
       
        
       
         <div class="form-group">
            <label asp-for="Description"><b>Тавсиф</b></label>
            <input type="text" asp-for="Description" class="form-control" />
            <span asp-validation-for="Description"></span>
        </div>
     
         
        <input asp-for="Image" type="hidden"  class="form-control" accept="application" />
         
         
       
        <div class="form-group">
            <label asp-for="CreatedAt"><b>Рузи бромад</b></label>
            <input type="text" asp-for="CreatedAt" class="form-control" />
            <span asp-validation-for="CreatedAt"></span>
        </div>   
         <div class="form-group">
            <label asp-for="Enabled"><b>Фаьол</b></label>
            <input type="text" asp-for="Enabled" class="form-control" />
            <span asp-validation-for="Enabled"></span>
        </div>   
       
       
       <div class="card-footer">
                <center>
                    <button type="submit" class="btn btn-primary">Нест намудан</button>
                    <a asp-action="Index" asp-controller="News" class="btn btn-secondary">Бекор намудан</a>
                </center>
            </div>
    </div>
</form>