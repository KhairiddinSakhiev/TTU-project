@using Domain.EntitiesDto
@model DepartmentImageDto


<h1>Вы точно хотите удалить</h1>
<form asp-action="Delete"  method="post" asp-controller="DepartmentImage">
    <div class="row">
        <div class="col-lg-12">
            <div class="form-group">
                <label>Title:</label>
                <input asp-for="Title" type="text" class="form-control " placeholder="Введите названия" tabindex="1">
            </div>
          
                
                <input asp-for="DepartmentId" type="hidden" class="form-control">
           
                      
            
           
            <div class="form-group">
                <div asp-validation-summary="All" class="text-danger"></div>
            </div>

            <div class="card-footer">
                <center>
                    <button type="submit" class="btn btn-primary">Изменить</button>
                    <a asp-action="Index" asp-controller="DepartmentImage" class="btn btn-secondary">Оменить</a>
                </center>
            </div>
        </div>
    </div>
</form>


